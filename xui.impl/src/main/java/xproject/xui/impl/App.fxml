<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TreeItem?>

<BorderPane cache="true" focusTraversable="true" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xproject.xui.impl.AppController">
   <top>
   		<MenuBar>
            <menus>
   	         	<Menu text="Registry">
      				<items>
      	         		<MenuItem fx:id="newRegistry" onAction="#onNewRegistry" text="connect" />
      				</items>
   	         	</Menu>
            </menus>
       	</MenuBar>
   </top>
   <center>
      <Pane cache="true" focusTraversable="true" mouseTransparent="true" prefHeight="600.0" prefWidth="800.0" BorderPane.alignment="TOP_LEFT">
         <children>
            <TreeView fx:id="treeView" cache="true" editable="true" mouseTransparent="true" prefWidth="200.0" >
            	<root>
            		<TreeItem fx:id="root" >
            		</TreeItem>
            	</root>
            </TreeView>
         	
         </children>
      </Pane>
   </center>
</BorderPane>
