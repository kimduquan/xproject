@model XWebApplication.Models._XThisModel
@using XSystem
@using XWebApplication.Models.XSystem

<script type="text/javascript">
    function xondragstart(ev, type, hashCode) {
        ev.dataTransfer.setData("type", type);
        ev.dataTransfer.setData("hashCode", hashCode);
    }
</script>
<a href="/return">Sign out</a>
<ul>
    @foreach (KeyValuePair<string, XObject> v in Model.XCache.XObjects)
    {
        <li>
            <a href="@_XObjectModel.XToHref(v.Value)" draggable="true" ondragstart="xondragstart(event, '@v.Value.XGetType().XFullName', '@v.Value.XGetHashCode()')">@_XObjectModel.XToString(v.Value)</a>
        </li>
    }
</ul>