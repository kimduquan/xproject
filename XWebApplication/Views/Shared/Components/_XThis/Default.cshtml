@model XWebApplication.Models._XThisModel
@using XSystem
@using XWebApplication.Models.XSystem

<script type="text/javascript">
    function xondragstart(ev, key) {
        ev.dataTransfer.setData("this", key);
    }
    function xondrop(ev, target) {
        ev.preventDefault();
        var key = ev.dataTransfer.getData("this");
        if (key != null) {
            if (target != null) {
                target.value = key;
            }
        }
    }
    function xondragover(ev) {
        ev.preventDefault();
    }
</script>
<a href="/return">Sign out</a>
<ul>
    @foreach (KeyValuePair<string, XObject> v in Model.XCache.XObjects)
    {
        <li>
            <a href="@_XObjectModel.XToHref(v.Value)" draggable="true" ondragstart="xondragstart(event, '@v.Key')">@_XObjectModel.XToString(v.Value)</a>
        </li>
    }
</ul>