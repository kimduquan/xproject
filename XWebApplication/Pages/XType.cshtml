@page "/{assembly:regex(^[A-Za-z0-9-_]+$)}/{namespace:regex(^[A-Za-z0-9-_]+$)}/{type:regex(^[A-Za-z0-9_]+$)}"
@model XTypeModel
@{
    ViewData["Title"] = @Model.XType.XName;
}

<nav>
    @foreach (var xmethod in Model.XStaticMethods)
    {
        string href = Model.XGetHref(xmethod);
        <a href="@href"></a>
    }
</nav>
<ul>
    @foreach (var xfield in Model.XArrayFields)
    {

    }
</ul>
@await Html.PartialAsync("_XSystem/_XReflection/_XAssembly", Util.XGetAssembly(Model.XType));
@await Html.PartialAsync("_XSystem/_XType", Model.XType);